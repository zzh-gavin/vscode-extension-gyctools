{
	"name": "gyctools",
	"displayName": "Generate Your Codes From DataBase",
	"description": "Generate Your Codes From DataBase by Nunjuck Templating.",
	"publisher": "zzh-gavin",
	"author": "zzh-gavin<leechzhao3@hotmail.com>",
	"version": "0.0.1",
	"license": "MIT",
	"preview": false,
	"engines": {
		"vscode": "^1.62.0"
	},
	"categories": [
		"Other",
		"Snippets",
		"Programming Languages"
	],
	"keywords": [
		"mysql",
		"database",
		"dto",
		"ormap",
		"ormapping",
		"gen",
		"generate"
	],
	"icon": "icon/icon-256.png",
	"extensionDependencies": [
		"mtxr.sqltools"
	],
	"activationEvents": [
		"onLanguage:java",
		"onView:sqltoolsViewConnectionExplorer"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "gyctools.generateYourCodes",
				"title": "Generate Your Codes"
			}
		],
		"menus": {
			"view/item/context": [
				{
					"command": "gyctools.generateYourCodes",
					"when": "view == sqltoolsViewConnectionExplorer && viewItem =~ /^connection\\.(table|view)$/",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/mysql": "^2.15.19",
		"@types/node": "14.x",
		"@types/nunjucks": "^3.2.0",
		"@types/vscode": "^1.62.0",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"@vscode/test-electron": "^1.6.2",
		"eslint": "^7.32.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.1",
		"typescript": "^4.4.3"
	},
	"dependencies": {
		"mysql": "^2.18.1",
		"nunjucks": "^3.2.3"
	}
}
